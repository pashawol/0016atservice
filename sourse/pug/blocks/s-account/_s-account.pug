mixin s-account()
	// start s-account
	+b.SECTION.s-account.section#s-account&attributes(attributes)
		.container
			+b.section-title
				h2 Персональный кабинет
			+b.tabs
				.row
					.col-lg-2
						.tabs__caption
							.tabs__btn.active Учетная запись
							.tabs__btn Заказы
							.tabs__btn Специальные предложения
							.tabs__btn Просмотренные товары
							.tabs__btn Настройки
					.tabs__wrap.col-lg-10
						.tabs__content.active
							+b.personal-info
								h3 Name Name
								span.acd_addr address
								<iframe src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d10891034.737036085!2d36.25293163462091!3d48.20769403315639!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1sru!2sua!4v1579532013946!5m2!1sru!2sua" height="350" frameborder="0" style="border:0;" allowfullscreen=""></iframe>
								.hdr#reg_h_company
									p Контактная информация:
								.c_data_phone
									h5 Телефоны
									.cd_phone
										span.cd_phone_num Основной номер
										span.cd_phone_comment +3808000001
								.c_data_email
									h5 Электронная почта
									.cd_phone
									span.cd_phone_num Основной email (логин)
									span.cd_phone_comment ________@gmail.com
						.tabs__content
							<div class="cab_container">
								<div style="display: none;" id="s_r" class="sel_result">Заказы: <b><div id="sel_result" style="display: none;"></div></b> <input type="button" onclick="_MoveStart()" value="Переместить" class="formbutton_warn"><input type="button" value="Печать" onclick="_printSelOrders()" class="formbutton_stop"><!--<input type="button" value="Сообщение" class="formbutton_stop" />--><input type="button" onclick="_cancel()" value="Отмена" class="formbutton"></div><div style="display: none;" id="s_d" class="sel_result"><b>Выберите новое расположение</b> <input type="button" onclick="_MoveCancel()" value="Отмена" class="formbutton"></div><input id="sel_arr" type="hidden"><div id="selectable"></div>
								<script type="text/javascript">InitTriggers(); InitControls(); </script></div>
						.tabs__content
							<script type="text/javascript">InitTriggers(); InitControls(); </script>
						.tabs__content
							| История просмотров пуста
							<script type="text/javascript">InitTriggers(); InitControls(); </script>
						.tabs__content
							<div class="cab_container" style="display: block;"><div class="ui-widget" id="b_ms_info">
								<div class="ui-state-highlight ui-corner-all" style="margin-top: 20px; padding: 0 .7em; font-weight: normal; display: none;">
									<p><span class="ui-icon ui-icon-info" style="float: left; margin-right: .3em;"></span>
									</p><p id="ms_info">  </p>
								</div>
								
								<div class="ui-widget" style="font-weight: normal;" id="b_ms_warn">
									<div class="ui-state-error ui-corner-all" style="padding: 0 .7em; display: none">
										<p><span class="ui-icon ui-icon-alert" style="float: left; margin-right: .3em;"></span>
										</p><p id="ms_warn">  </p>
									</div>
								</div>


							<table cellpadding="0" cellspacing="3" border="0" width="100%">

							<tbody><tr>
								<td> Фамилия:
									<br>
									<div class="ui-corner-all ui-widget-control"><input id="secondname" value="" class="ui-corner-all" style="z-index: 100; position: relative; width: 210px;" title="type &quot;a&quot;"></div>	
								</td>
								<td> Имя:
									<br>
									<div class="ui-corner-all ui-widget-control"><input id="firstname" value="" class="ui-corner-all" style="z-index: 100; position: relative; width: 210px;" title="type &quot;a&quot;"></div>	
								</td>
								<td> Отчество:
									<br>
									<div class="ui-corner-all ui-widget-control"><input id="lastname" value="" class="ui-corner-all" style="z-index: 100; position: relative; width: 210px;" title="type &quot;a&quot;"></div>	
								</td>
							</tr>
							<tr>
								<td> Страна:
									<br>		
									<div class="ui-corner-all ui-widget-control"><input id="secondname" value="" class="ui-corner-all" style="z-index: 100; position: relative; width: 210px;" title="type &quot;a&quot;"></div>	
								</td>
								<td> Регион:
									<br>
									<div class="ui-corner-all ui-widget-control"><input id="secondname" value="" class="ui-corner-all" style="z-index: 100; position: relative; width: 210px;" title="type &quot;a&quot;"></div>	
								</td>
								<td> Город:
									<br>
									<div class="ui-corner-all ui-widget-control"><input id="secondname" value="" class="ui-corner-all" style="z-index: 100; position: relative; width: 210px;" title="type &quot;a&quot;"></div>	
								</td>
							</tr>

							</tbody></table>

							<table cellpadding="0" cellspacing="3" border="0" width="100%">
							<tbody><tr>
								<td> Адрес:
									<br>
									<div class="ui-corner-all ui-widget-control"><input id="addr" value="" class="ui-corner-all" style="z-index: 100; position: relative; width: 470px;" title="type &quot;a&quot;"></div>	
								</td>
								<td> Телефон:
									<br>
									<div class="ui-corner-all ui-widget-control"><input id="phone" value="" class="ui-corner-all" style="z-index: 100; position: relative; width: 150px;" title="type &quot;a&quot;"></div>	
								</td>
							</tr>
							</tbody></table>

							<table cellpadding="0" cellspacing="3" border="0" width="100%">
							<tbody><tr>
								<td> email:
									<span style="font-weight: normal; font-size: 11px;">(используется как логин)</span><br>
									<div class="ui-corner-all ui-widget-control"><input disabled="disabled" value="" id="email" class="ui-corner-all" style="z-index: 100; position: relative; width: 320px;" title="type &quot;a&quot;"></div>	
								</td>
								<td> Вебсайт:
									<br>
									<div class="ui-corner-all ui-widget-control"><input id="website" value="" class="ui-corner-all" style="z-index: 100; position: relative; width: 320px;" title="type &quot;a&quot;"></div>	
								</td>
							</tr>
							</tbody></table>

							<center>
							<div style="display: inline-block; text-align: left; margin-top: 30px;"> Старый пароль:
							<br>
							<div class="ui-corner-all ui-widget-control" style="border-color: red;"><input type="password" id="oldpass" class="ui-corner-all" style="z-index: 100; position: relative; width: 320px;" title="type &quot;a&quot;"></div><br> Новый пароль:
							<br>
							<div class="ui-corner-all ui-widget-control" style="border-color: orange;"><input type="password" id="newpass" class="ui-corner-all" style="z-index: 100; position: relative; width: 320px;" title="type &quot;a&quot;"></div><br> Подтверждение:
							<br>
							<div class="ui-corner-all ui-widget-control" style="border-color: orange;"><input type="password" id="newconfirm" class="ui-corner-all" style="z-index: 100; position: relative; width: 320px;" title="type &quot;a&quot;"></div>
							</div>
							<br>
							<br>
							<button id="button" onclick="save()">Сохранить</button>
							</center><script type="text/javascript">InitTriggers(); InitControls(); </script></div>
	// end s-account