			block
			.new_items 
				+prod-item()
				+prod-item('content/proc/sml_img49973054449.jpg')
				+prod-item('content/proc/sml_img18955398392.jpg')
				+prod-item('content/proc/sml_img38569715535.jpg')
	// end s-catalog